#config parameters

[I/O]
#file names
template_file = /home/ckrawiec/DES/data/balrog_sva1_TRUTH_zp_corr_fluxes_fixids.fits
target_file = /home/ckrawiec/DES/data/balrog_sva1_tab05_TRUTH_zp_corr_SVnoised_fluxes.fits
output_file = /home/ckrawiec/DES/magnification/lbgselect/zproboutput/balrog_sva1_flux0_SVnoised_griz_z3_2bins_4tree_tab05_culltree_debug.fits

[debugging]
#debug parameters
debug = True
N_debug = 300

[parameters]
#bands to use, same case as column names require
filters = GRIZ

#redshift groups for which to calculate probability
#of target membership
redshift_ranges = [[3.0, 9.9]]

#number of threads for multiprocessing
num_threads = 1
#'tree' or 'full' integration over templates
integration = tree
query_radius = 4.

[data]
#target column names, case sensitive
target_id_column = BALROG_INDEX
target_data_column = FLUX_0+NOISE_{}
target_error_column = FLUXERR_0+NOISE_{}

#template column names, case sensitive
template_id_column = BALROG_INDEX
template_data_column = FLUX_0_{}
redshift_column = Z

#specify indices to use slice of target data
#if not specified, all are used
target_start_index = 0
target_end_index = 10000
